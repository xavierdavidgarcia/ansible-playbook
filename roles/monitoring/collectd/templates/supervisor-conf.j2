[program:{{APP_NAME}}]
process_name={{APP_NAME}}
command={{APP_FOLDER}}/{{APP_NAME}}/sbin/collectd -C {{CONF_FOLDER}}/{{APP_NAME}}/collectd.conf -f -P /var/run/collectd.pid
numprocs=1                    ; number of processes copies to start (def 1)
autostart=true                ; start at supervisord start (default: true)
autorestart=true              ; retstart at unexpected quit (default: true)
startsecs=60                  ; number of secs prog must stay running (def. 1)
startretries=3                ; max # of serial start failures (default 3)
stderr_logfile = {{LOG_FOLDER}}/{{APP_NAME}}/{{APP_NAME}}-stderr.log
stdout_logfile = {{LOG_FOLDER}}/{{APP_NAME}}/{{APP_NAME}}-stdout.log